    COMPRESS_POSES   YES
    EPIK_PENALTIES   YES
    FORCEPLANAR   NO
[USEROUTS:$task]
    USEROUTS   XP_OUT,
    STRUCTOUT   XP_OUT
XP
    Ligand_name="XP_OUT"
}

XP_local(){
    TO_RMSD="${Ligand_name}"
    KEEP_NUM=`parse_num_or_percentage ${Docking_out_num}`
cat<<XP >> ${Inp_Name}
[STAGE:PRE_DOCK_XP]
    STAGECLASS   gencodes.RecombineStage
    INPUTS   ${Ligand_name},
    OUTPUTS   DOCK_XP_INPUT,
    NUMOUT   njobs
    OUTFORMAT   maegz
    MIN_SUBJOB_STS   300
    MAX_SUBJOB_STS   5000
    GENCODES   NO
    UNIQUEFIELD   s_vsw_compound_code
[STAGE:DOCK_XP]
    STAGECLASS   glide.DockingStage
    INPUTS   DOCK_XP_INPUT, GRID
    OUTPUTS   XP_OUT,
    RECOMBINE   NO
    PRECISION   XP
    REWARD_INTRA_HBONDS YES
    HBOND_ACCEP_HALO    YES
    MAXKEEP 3000
    MAXREF 300
    UNIQUEFIELD   s_vsw_compound_code
    ${KEEP_NUM}
    FORCEFIELD  ${Force_Field}
    DOCKING_METHOD   mininplace
    POSES_PER_LIG   ${Dock_out_conf}
    WRITE_XP_DESC   NO
    BEST_BY_TITLE   NO
    LIG_VSCALE   0.8
    LIG_CCUT   0.15
    MAXATOMS   300
    MAXROTBONDS   50
    AMIDE_MODE   penal
    POSE_OUTTYPE   PV
    POSTDOCK   YES
    POSTDOCKSTRAIN   NO
    COMPRESS_POSES   YES
    EPIK_PENALTIES   YES
    FORCEPLANAR   NO
[USEROUTS:$task]
    USEROUTS   XP_OUT,
    STRUCTOUT   XP_OUT
XP
    Ligand_name="XP_OUT"
}

